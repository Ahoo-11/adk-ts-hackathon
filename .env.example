# WeatherWise MCP - Environment configuration
# Copy to .env and fill in your API keys

# Server
PORT=3000

# Weather Providers
OPENWEATHER_API_KEY=your_openweathermap_api_key
WEATHERAPI_API_KEY=your_weatherapi_com_api_key

# Default settings
DEFAULT_UNITS=metric # metric | imperial
CACHE_TTL_SECONDS=600
FORECAST_CACHE_TTL_SECONDS=10800
ALERT_POLL_INTERVAL_SECONDS=600

# Optional: Google Gemini API key for ADK-TS agent examples
GOOGLE_API_KEY=your_google_api_key